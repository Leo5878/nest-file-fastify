{
  "name": "@blazity/nest-file-fastify",
  "version": "1.0.0",
  "description": "fastify-multipart decorators for Nest.js",
  "license": "MIT",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/976977577/nest-file-fastify"
  },
  "keywords": [
    "nest",
    "nestjs",
    "fastify",
    "multipart",
    "@fastify/multipart",
    "upload",
    "file",
    "nest-file"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "concurrently \"tsc --watch --p tsconfig.lib.json\" \"tsc --watch --p tsconfig.examples.json\"",
    "watch": "nodemon ./build/examples/index.js",
    "build:ci": "tsc --p tsconfig.lib.json && tsc --p tsconfig.examples.json",
    "build": "tsc --p tsconfig.lib.json",
    "lint": "eslint \"src/**\" --ext \".js,.jsx,.ts,.tsx\"",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf build",
    "prepublishOnly": "yarn clean && npm run build"
  },
  "devDependencies": {
    "@fastify/multipart": "^7.7.3",
    "@nestjs/common": "^11.1.14",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.14",
    "@nestjs/platform-fastify": "^11.1.14",
    "@types/busboy": "^1.5.0",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.0",
    "fastify": "^5.7.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.3",
    "rxjs": "^7.8.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@fastify/multipart": "^9.4.0"
  }
}
